{
  "name": "GetIT",
  "version" : "0.0.1",
  "description" : "A service to help purchase items from images you select",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}