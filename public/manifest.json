{
  "manifest_version": 2,
  "name": "Get It",
  "version": "1.0",
  "description": "A service to help purchase items from images you select",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "index.html"
  }
}
