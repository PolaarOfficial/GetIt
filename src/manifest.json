{
  "manifest_version": 2,
  "name": "Get It",
  "version": "1.0.0",
  "description": "Capture and download webpage screenshots.",
  "permissions": ["tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "inactive.png",
    "default_title": "Take screenshot"
  },
  "web_accessible_resources": ["active.png", "inactive.png"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
