{
  "manifest_version": 2,
  "name": "Get It",
  "version": "1.0.0",
  "description": "Capture and download webpage screenshots.",
  "permissions": ["tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Take screenshot"
  }
}
